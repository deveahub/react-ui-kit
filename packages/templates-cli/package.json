{
	"name": "@rrios-dev/templates-cli",
	"version": "0.0.3",
	"license": "MIT",
	"main": "dist/main.js",
	"bin": {
		"templates-cli": "dist/main.js"
	},
	"engines": {
		"node": ">=10"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rrios-dev/react-ui-kit"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/rrios-dev"
	},
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc --watch",
		"dev": "nodemon --watch src --exec node dist/main.js ",
		"start": "node dist/main.js",
		"pretest": "npm run build",
		"copy:templates": "node scripts/copyTemplates.js",
		"postbuild": "(echo '#! /usr/bin/env node' && cat dist/main.js) > dist/main2.js && mv dist/main2.js dist/main.js && chmod +x dist/main.js && npm run copy:templates",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist/**"
	],
	"dependencies": {
		"ink": "^3.2.0",
		"ink-select-input": "^4.2.1",
		"ink-text-input": "^4.0.3",
		"meow": "^10.1.3",
		"react": "^18.1.0",
		"undefined": "^0.1.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.9",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"chalk": "^5.0.1",
		"@rrios-dev/node-config": "*",
		"eslint": "8.20.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.2.7",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.6.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"ink-testing-library": "^2.1.0",
		"nodemon": "^2.0.16",
		"typescript": "^4.7.2"
	}
}
